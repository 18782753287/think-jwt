{
    "name": "xiaodi/think-jwt-auth",
    "type": "think-extend",
    "license": "MIT",
    "keywords": [
        "php",
        "thinkphp"
    ],
    "description": "ThinkPHP JwtAuth Component",
    "authors": [
        {
            "name": "xiaodi",
            "email": "liangjinbiao@live.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "lcobucci/jwt": "^3.3",
        "nette/php-generator": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "xiaodi\\": "src/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "xiaodi\\JwtAuthService"
            ],
            "config": {
                "jwt-auth": "config/config.php"
            }
        },
        "think-config": {
            "jwt-auth": "config/config.php"
        }
    }
}
